{% extends "./main.twig" %}
{% block main %}
	<section class="container-fluid">
	<div class="row ">
	{% for project in projects %}
	
			
				<div class="fond-card m-3 p-2 offset-1 col-lg-3 col-mg-2" style="text-align: center">
					<div class="card-body">
						<h5 class="card-title">{{project.nameProject}}</h5>
						<img src="/uploads/images/{{project.image}}" class="img-fluid" >
						<p class="card-text">{{project.discribProject}}</p>
						<p class="card-text">{{project.githubProject}}</p>
					</div>
				</div>
			
		


	{% endfor %}
	</div>
	</section>
{% endblock %}
